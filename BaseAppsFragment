/**
 * BaseMediaFragment is base class for NowPlayingMediaFragment and
 * RadioBrowseFragment and contains functionalities common to those fragments
 * 
 * @author Harman Automotive
 */
public abstract class BaseAppsFragment extends BaseFragment {



	@Override
	public void onStart() {
		super.onStart();
	}

	@Override
	public void onPause() {
		super.onPause();

	}

	@Override
	public void onResume() {
		super.onResume();
	}

	public void setLastAppsSourceMode() {
		Logger.d("BaseAppsFragment", "setLastAppsSourceMode");
	}
}
